          <script>
		var app = angular.module('myApp', []);
			  app.controller('switchCtrl' ,function($scope) {
				  $scope.navHome = 1;
				  $scope.clickHome = function(){
					   $scope.navHome = 1;
					   $scope.navAbout = 0;
					   $scope.navContact = 0;
					   $scope.navBlog = 0;
				  };
				  $scope.clickAbout = function(){
					   $scope.navHome = 0;
					   $scope.navAbout = 1;
					   $scope.navContact = 0;
					   $scope.navBlog = 0;
				  };
				  $scope.clickContact = function(){
					   $scope.navHome = 0;
					   $scope.navAbout = 0;
					   $scope.navContact = 1;
					   $scope.navBlog = 0;
				  };
				  $scope.clickBlog = function(){
					   $scope.navHome = 0;
					   $scope.navAbout = 0;
					   $scope.navContact = 0;
					   $scope.navBlog = 1;
				  };
			  });
			  
	/*		  var arrayPics = ["images/page-layouts-1408107.jpg","images/page-layouts-3112451.jpg","images/page-layouts-5658534.jpg"];
			  
			  for (var x=0, x < arrayPics.length, x++){
				  
				  iHtml += '<div class="col-lg-4"><img'  height="140" width="140"></div>"
				  
				  document.getElementById("id").innerHTML = iHtml;
			  }
			  
			  */
		</script>
        
      
